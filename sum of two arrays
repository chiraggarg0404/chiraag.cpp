#include <iostream>
#include <math.h>
using namespace std;

int main() {
    int n,a[3][3],b[3][3],sum[3][3],i,j;
cout << "enter array 1";
for(i=0;i<3;i++)
{
    for(j=0;j<3;j++)
    {
        cin>> a[i][j];
    }
}
cout << "enter array 2";
for(i=0;i<3;i++)
{
    for(j=0;j<3;j++)
    {
        cin>> b[i][j];
        sum[i][j]=a[i][j]+b[i][j];
    }
}
for(i=0;i<3;i++)
{
    for(j=0;j<3;j++)
    {
        cout<<endl<<sum[i][j];
    }
}
    return 0;
}
