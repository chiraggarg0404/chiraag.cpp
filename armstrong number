#include <iostream>
#include <math.h>
using namespace std;

int main() {
    int n,y,sum=0,t;
    cout << "enter the number";
    cin >> n;
    t=n;
    while(n!=0)
    {y=n%10;
    sum=sum+y*y*y;
    n=n/10;
    }
    if(sum==t)
    cout<< "armstrong number";
    else
    cout<< "not an armstrong number";

    return 0;
}
